{
  "name": "lobster-sage",
  "version": "0.1.0",
  "description": "Autonomous OpenClaw agent for predictions and yield farming on Base",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "compile:contracts": "hardhat compile",
    "deploy:sepolia": "hardhat run scripts/deploy.ts --network baseSepolia",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network baseMainnet",
    "verify": "hardhat verify"
  },
  "keywords": [
    "openclaw",
    "base",
    "defi",
    "agent",
    "prediction",
    "yield"
  ],
  "author": "kcpele",
  "license": "MIT",
  "dependencies": {
    "@coinbase/cdp-agentkit-core": "^0.0.13",
    "@coinbase/cdp-langchain": "^0.0.14",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.0",
    "viem": "^2.21.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox-viem": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.0",
    "hardhat": "^2.22.0",
    "prettier": "^3.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.0",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
